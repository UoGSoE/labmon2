{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ohnotnow/laravel-prometheus-exporter"
        }
    ],
    "require": {
        "php": "^8.1",
        "appstract/laravel-options": "^5.4",
        "ohnotnow/laravel-prometheus-exporter": "^2.0",
        "laravel/framework": "^10.34",
        "laravel/horizon": "^5.12",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "livewire/livewire": "^2.11",
        "sentry/sentry-laravel": "^3.2",
        "titasgailius/terminal": "^1.0.1",
        "uogsoe/ldap-laravel": "^3.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "brianium/paratest": "^6.2",
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.13",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0",
        "spatie/test-time": "^1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": []
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
